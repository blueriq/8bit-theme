{
  "name": "blueriq-8bit",
  "version": "1.0.0",
  "license": "MIT",
  "homepage": "https://github.com/blueriq/blueriq-8bit",
  "keywords": [
    "blueriq",
    "theme",
    "8bit"
  ],
  "scripts": {
    "ng": "ng",
    "ng:version": "ng --version",
    "start": "ng serve --port 8888 --open --host 0.0.0.0 --proxy-config proxy.conf.js",
    "build": "ng build --prod",
    "build:runtime": "ng build --prod -c runtime",
    "test": "ng test",
    "coverage": "ng test --watch false --code-coverage true",
    "tslint": "ng lint",
    "sass-lint": "sass-lint --verbose --config sass-lint.yml src/**/*.scss",
    "lint": "yarn tslint && yarn sass-lint",
    "verify": "yarn coverage && yarn tslint && yarn sass-lint",
    "e2e": "yarn protractor e2e/protractor.conf.js",
    "e2e-dev": "yarn webdriver-manager update && protractor e2e/protractor-dev.conf.js",
    "version:increment": "npm version --allow-same-version"
  },
  "engines": {
    "yarn": "^1.4.0"
  },
  "dependencies": {
    "@angular/animations": "7.0.2",
    "@angular/cdk": "7.0.2",
    "@angular/common": "7.0.2",
    "@angular/compiler": "7.0.2",
    "@angular/core": "7.0.2",
    "@angular/flex-layout": "7.0.0-beta.19",
    "@angular/forms": "7.0.2",
    "@angular/platform-browser": "7.0.2",
    "@angular/platform-browser-dynamic": "7.0.2",
    "@angular/router": "7.0.2",
    "@blueriq/angular": "0.6.7",
    "@blueriq/core": "0.6.7",
    "@ngrx/effects": "6.1.0",
    "@ngrx/store": "6.1.0",
    "@ngrx/store-devtools": "6.1.0",
    "chart.js": "2.7.3",
    "core-js": "2.5.7",
    "font-awesome": "4.7.0",
    "hammerjs": "2.0.8",
    "moment": "2.22.2",
    "nes.css": "1.0.0",
    "ng-pick-datetime": "6.0.5",
    "ng-pick-datetime-moment": "1.0.6",
    "ng2-file-upload": "1.3.0",
    "ngx-toastr": "^9.1.1",
    "rxjs": "6.3.3",
    "rxjs-compat": "6.3.3",
    "typeface-press-start-2p": "0.0.54",
    "web-animations-js": "2.3.1",
    "yarn": "^1.13.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.10.4",
    "@angular/cli": "7.0.2",
    "@angular/compiler-cli": "7.0.2",
    "@angular/language-service": "7.0.2",
    "@compodoc/compodoc": "1.1.6",
    "@types/jasmine": "2.8.9",
    "@types/jasminewd2": "2.0.5",
    "@types/node": "10.11.6",
    "codelyzer": "4.5.0",
    "jasmine-core": "3.2.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "3.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.3.1",
    "karma-junit-reporter": "1.2.0",
    "protractor": "5.4.1",
    "protractor-jasmine2-html-reporter": "0.0.7",
    "sass-lint": "1.12.1",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.1.6",
    "webdriver-manager": "12.1.0"
  }
}
